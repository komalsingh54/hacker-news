{
  "name": "hackernews",
  "version": "0.10.1",
  "description": "The simplest Async Universal React & Redux boilerplate.",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint",
    "lint-js": "eslint .",
    "lint": "npm run lint-js",
    "fix-js": "npm run lint-js --fix",
    "fix": "npm run fix-js",
    "dev-server": "BABEL_ENV=server NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": " NODE_ENV=development webpack --config ./scripts/webpack.config.dev.js",
    "dev": "concurrently \"NODE_ENV=development npm run dev-server\" \"NODE_ENV=development npm run dev-client\"",
    "prod-server": "BABEL_ENV=server NODE_ENV=production webpack --config ./scripts/webpack.config.server.prod.js",
    "prod-client": "NODE_ENV=production webpack --config ./scripts/webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start": "NODE_ENV=production node ./dist/server/index.js",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "test": "jest --coverage=true"
  },
  "author": "Komal Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/komalsingh54/react-ssr"
  },
  "homepage": "https://github.com/komalsingh54/react-ssr",
  "keywords": [
    "hacker news"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint . --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-spread": "^7.8.3",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
